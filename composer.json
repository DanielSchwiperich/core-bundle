			"src/Resources/forms/",
			"src/Resources/hybrids/",
			"src/Resources/maintenance/",
			"src/Resources/models/",
			"src/Resources/modules/",
			"src/Resources/pages/",
			"src/Resources/uploader/",
			"src/Resources/widgets/"
